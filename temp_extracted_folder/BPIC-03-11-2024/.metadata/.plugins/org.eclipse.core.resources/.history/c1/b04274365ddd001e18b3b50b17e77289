package com.humana.bpic.DEPEnrollmentValidation.request;

public class SpecialElectionPeriodException {
	private String specialElectionPeriodReasonType="";
	private String disasterSpecialElectionPeriodReasonType="";
	private String disasterSpecialElectionPeriodDate="";
	private String disasterSpecialElectionPeriodEmergencyReason="";
	private String specialElectionPeriodDate="";
	private String specialElectionPeriodReason="";
	private String disasterElectionType="";
	private String disasterSpecialElectionPeriodReason="";
	
	public String getSpecialElectionPeriodReasonType() {
		return specialElectionPeriodReasonType;
	}
	public void setSpecialElectionPeriodReasonType(String specialElectionPeriodReasonType) {
		this.specialElectionPeriodReasonType = specialElectionPeriodReasonType;
	}
	
	public String getDisasterSpecialElectionPeriodReasonType() {
		return disasterSpecialElectionPeriodReasonType;
	}
	public void setDisasterSpecialElectionPeriodReasonType(String disasterSpecialElectionPeriodReasonType) {
		this.disasterSpecialElectionPeriodReasonType = disasterSpecialElectionPeriodReasonType;
	}
	public String getDisasterSpecialElectionPeriodDate() {
		return disasterSpecialElectionPeriodDate;
	}
	public void setDisasterSpecialElectionPeriodDate(String disasterSpecialElectionPeriodDate) {
		this.disasterSpecialElectionPeriodDate = disasterSpecialElectionPeriodDate;
	}
	public String getDisasterSpecialElectionPeriodEmergencyReason() {
		return disasterSpecialElectionPeriodEmergencyReason;
	}
	public void setDisasterSpecialElectionPeriodEmergencyReason(String disasterSpecialElectionPeriodEmergencyReason) {
		this.disasterSpecialElectionPeriodEmergencyReason = disasterSpecialElectionPeriodEmergencyReason;
	}
	public String getSpecialElectionPeriodDate() {
		return specialElectionPeriodDate;
	}
	public void setSpecialElectionPeriodDate(String specialElectionPeriodDate) {
		this.specialElectionPeriodDate = specialElectionPeriodDate;
	}

	public String getSpecialElectionPeriodReason() {
		return specialElectionPeriodReason;
	}
	public void setSpecialElectionPeriodReason(String specialElectionPeriodReason) {
		this.specialElectionPeriodReason = specialElectionPeriodReason;
	}
	public String getDisasterElectionType() {
		return disasterElectionType;
	}
	public void setDisasterElectionType(String disasterElectionType) {
		this.disasterElectionType = disasterElectionType;
	}
	public String getDisasterSpecialElectionPeriodReason() {
		return disasterSpecialElectionPeriodReason;
	}
	public void setDisasterSpecialElectionPeriodReason(String disasterSpecialElectionPeriodReason) {
		this.disasterSpecialElectionPeriodReason = disasterSpecialElectionPeriodReason;
	}
	

}

<?xml version="1.0" encoding="UTF-8"?>
<ilog.rules.studio.model.brl:ActionRule xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:ilog.rules.studio.model.brl="http://ilog.rules.studio/model/brl.ecore">
  <name>Date of Birth is not in ISO Format</name>
  <uuid>3c5a06a0-f9b3-4bef-94fb-bd32cf94001b</uuid>
  <locale>en_US</locale>
  <definition><![CDATA[definitions
    set 'Date of Birth' to the date of birth date of the consumers of 'the odm consumer data request' ;
    set 'DOB' to remove special characters and space ( 'Date of Birth' ) ;
if
    'DOB' is not null
    and 'DOB' is not empty
    and ( Numeric validation of 'DOB' is true
    and the length of String: 'DOB' is 8 )
    and Date format check of 'Date of Birth' is false
then
    add the error message with ruleset name - "Demographics - DOB" , issue code "P0000012" , action code "" , from value of 'Date of Birth' and to value of "" for the DOB issue of 'the odm consumer data response' ;]]></definition>
</ilog.rules.studio.model.brl:ActionRule>

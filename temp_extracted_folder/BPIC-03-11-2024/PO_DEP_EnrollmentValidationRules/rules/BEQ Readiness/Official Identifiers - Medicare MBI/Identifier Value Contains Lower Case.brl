<?xml version="1.0" encoding="UTF-8"?>
<ilog.rules.studio.model.brl:ActionRule xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:ilog.rules.studio.model.brl="http://ilog.rules.studio/model/brl.ecore">
  <name>Identifier Value Contains Lower Case</name>
  <uuid>a735a921-10ba-4ce0-92b2-c2fa0c97ad64</uuid>
  <categories>Medicare</categories>
  <locale>en_US</locale>
  <definition><![CDATA[definitions
    set 'official identifier' to an official identifiers in the official identifiers of the consumers of 'the odm consumer data request' ;
    set 'identifierCode' to the identifier code of 'official identifier' ;
    set 'identifierValue' to the identifier value of 'official identifier' ;
    set 'upperCase' to convert 'identifierValue' to upper case ;
if
    'identifierCode' is "MC"
    and 'identifierValue' is not null
    and 'identifierValue' is not empty
    and the length of String: 'identifierValue' is 11
    and is 'identifierValue' Lower Case
then
    set the identifier value of 'official identifier' to 'upperCase' ;
    add the error message with ruleset name - "Official Identifiers - Medicare MBI" , issue code "P0000005" , action code "A0000002" , from value of identifierValue and to value of the identifier value of 'official identifier' for the MBI issue of 'the odm consumer data response' ;]]></definition>
</ilog.rules.studio.model.brl:ActionRule>

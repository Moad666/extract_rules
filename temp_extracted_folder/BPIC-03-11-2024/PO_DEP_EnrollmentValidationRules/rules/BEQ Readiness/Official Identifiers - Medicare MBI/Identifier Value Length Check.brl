<?xml version="1.0" encoding="UTF-8"?>
<ilog.rules.studio.model.brl:ActionRule xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:ilog.rules.studio.model.brl="http://ilog.rules.studio/model/brl.ecore">
  <name>Identifier Value Length Check</name>
  <uuid>e0c171bb-2f67-49a9-a2a6-e33e914d6e62</uuid>
  <locale>en_US</locale>
  <definition><![CDATA[definitions
    set 'official identifier' to an official identifiers in the official identifiers of the consumers of 'the odm consumer data request' ;
    set 'identifierCode' to the identifier code of 'official identifier' ;
    set 'identifierValue' to the identifier value of 'official identifier' ;
if
    'identifierCode' is "MC"
    and 'identifierValue' is not null
    and 'identifierValue' is not empty
    and the length of String: 'identifierValue' is not 11
then
    add the error message with ruleset name - "Official Identifiers - Medicare MBI" , issue code "P0000004" , action code "" , from value of identifierValue and to value of "" for the MBI issue of 'the odm consumer data response' ;]]></definition>
</ilog.rules.studio.model.brl:ActionRule>

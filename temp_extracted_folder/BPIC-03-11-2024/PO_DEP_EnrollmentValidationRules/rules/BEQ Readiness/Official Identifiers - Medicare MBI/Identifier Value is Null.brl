<?xml version="1.0" encoding="UTF-8"?>
<ilog.rules.studio.model.brl:ActionRule xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:ilog.rules.studio.model.brl="http://ilog.rules.studio/model/brl.ecore">
  <name>Identifier Value is Null</name>
  <uuid>6f56f8fa-4b56-452e-ae7d-b123f24fb997</uuid>
  <locale>en_US</locale>
  <definition><![CDATA[definitions
    set 'official identifier' to an official identifiers in the official identifiers of the consumers of 'the odm consumer data request' ;
    set 'identifierCode' to the identifier code of 'official identifier' ;
    set 'identifierValue' to the identifier value of 'official identifier' ;
if
    'identifierCode' is "MC"
    and ( 'identifierValue' is null
    or 'identifierValue' is empty )
then
    add the error message with ruleset name - "Official Identifiers - Medicare MBI" , issue code "P0000002" , action code "" , from value of identifierValue and to value of "" for the MBI issue of 'the odm consumer data response' ;]]></definition>
</ilog.rules.studio.model.brl:ActionRule>
